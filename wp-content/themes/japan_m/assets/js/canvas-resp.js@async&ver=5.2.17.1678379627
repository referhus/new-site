if (window.innerWidth < 768) {
    $('body').addClass('body-portrait');
    $('.city-portrait').show();
    $('.city-portrait img').attr('src', $('.city-portrait img').attr('attr-src'));
} else {
    $('body').addClass('body-landscape');
    $('.city-desktop').show();
    $('.city-desktop img').attr('src', $('.city-desktop img').attr('attr-src'));

}